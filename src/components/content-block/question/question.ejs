<div class="article-question">
    <div class="article-question__inner">
        <h2 class="content__title"><%= dataQuestion.title%></h2>
        <div class="article-question__items">
            <% dataQuestion.listQuestion.forEach(item => { %>
                <div class="article-question__container">
                    <div class="article-question__item" data-question-item>
                        <span class="article-question__title"><%= item.questionTitle%></span>
                        <svg class="article-question__icon" width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M11.9997 13.172L16.9497 8.22205L18.3637 9.63605L11.9997 16L5.63574 9.63605L7.04974 8.22205L11.9997 13.172Z" fill="#003C5A"/>
                        </svg>
                    </div>
                    <div class="article-question__block-text">
                        <p class="article-question__text content__text">
                            <%= item.questionText%>
                        </p>
                    </div>
                </div>
            <% }); %>
        </div>
    </div>
</div>